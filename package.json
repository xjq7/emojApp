{
  "name": "emojapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cross-env ENVFILE=.env.dev react-native run-android",
    "android:prod": "cross-env ENVFILE=.env.prod react-native run-android",
    "ios": "react-native run-ios",
    "releaseApk": "cd android && ./gradlew cleanBuildCache && cross-env ENVFILE=.env.prod ./gradlew assembleRelease",
    "releaseApk-dev": "cd android && ./gradlew cleanBuildCache && cross-env ENVFILE=.env.dev ./gradlew assembleRelease",
    "start": "react-native start",
    "restart": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --cache -f codeframe --fix",
    "pre-commit": "pretty-quick --staged",
    "pretty": "pretty-quick --staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/cameraroll": "https://github.com/xjq7/react-native-cameraroll.git",
    "@react-native-community/hooks": "^2.8.1",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "axios": "^0.24.0",
    "fbjs": "^3.0.2",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-config": "^1.4.5",
    "react-native-fs": "^2.18.0",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-haptic-feedback": "^1.13.0",
    "react-native-modal": "^13.0.0",
    "react-native-pager-view": "^5.4.9",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-toast-message": "^2.0.2",
    "react-native-vector-icons": "^9.0.0",
    "react-native-wechat-lib": "^1.1.26",
    "recoil": "^0.5.2",
    "recoil-nexus": "^0.3.9",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.178",
    "@types/react-native": "^0.66.4",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "7.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "6.0.0",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
